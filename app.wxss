/**app.wxss**/
.text_c{text-align: center;}
.col_gg{color: #1c7af9;}
.bg_gg{background-color: #1c7af9;}
.pad_rl15{padding-left: 15px;padding-right: 15px;}

.fixed-shareIt{
  padding-top:80rpx;
  position:fixed;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,.5);
  display:flex;
  align-items:center;
  justify-content: center;
}
.fixed-shareIt>view{
  position:absolute;
  width:80%;
  left:50%;
  margin-left:-40%;
}
.fixed-shareIt-close{
  width:52rpx;
  position:absolute;
  right:-48rpx;
  top:30rpx;
}
.fixed-shareIt-button{
  display:flex;
  justify-content: space-between;
}
.fixed-shareIt-button button{
  margin-top:30rpx;
  background-color:#0FA9FF;
  border-radius:12rpx;
  box-sizing: border-box;
  padding:20rpx !important;
  width:240rpx !important;
  display:flex;
  justify-content: center;
}
.fixed-shareIt-button>button.red{
  background-color:#FF4C69;
}
.fixed-shareIt-button-first{
  width:40rpx;
  margin-right:20rpx;
}
.fixed-shareIt-button-last{
  width:64rpx;
}
.index_bg{background: url(data:image/png;base64,)center center no-repeat;background-size: cover;}

.index_tips{
  background: url(data:image/png;base64,)center center no-repeat;background-size:100%;}

.index_list{background:url(data:image/png;base64,)center center no-repeat;background-size:cover;}